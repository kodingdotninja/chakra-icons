{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "# general commands": "--------------------------------------------------",
    "#": "-------------------------------------------------------------------"
  },
  "dependencies": {},
  "devDependencies": {
    "@kodingdotninja/tsconfig": "*",
    "eslint": "^8",
    "eslint-config-kdnj": "^2"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj/react"
    ],
    "ignorePatterns": [
      "node_modules"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "**/*.{js,cjs,mjs,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "license": "MIT"
}
