{
  "name": "create-chakra-icons",
  "version": "1.0.3-next.1",
  "author": "ri7nz <hi@rin.rocks>",
  "main": "./index.js",
  "bin": {
    "create-chakra-icons": "./index.bin.js"
  },
  "exports": {
    ".": "./index.js",
    "./ast": "./lib/ast.js",
    "./chakra": "./lib/chakra.js",
    "./cli": "./lib/cli.js",
    "./utils": "./lib/utils.js"
  },
  "files": [
    "*.js"
  ],
  "sideEffects": false,
  "scripts": {
    "# general commands": "--------------------------------------------------",
    "docs": "documentation readme lib/*.js -q --section API && yarn fixreadme",
    "fixreadme": "node -e 'var fs=require(\"fs\");fs.writeFileSync(\"README.md\", fs.readFileSync(\"README.md\", \"utf8\").replace(/^-   /gm, \"- \"))'",
    "format": "prettier --write \"**/*.{js,json,md,svg}\"",
    "lint": "eslint --fix \"**/*.js\"",
    "#": "-------------------------------------------------------------------"
  },
  "dependencies": {
    "@babel/generator": "^7",
    "@babel/types": "^7",
    "@svgr/hast-util-to-babel-ast": "^6",
    "change-case": "^4",
    "minimist": "^1",
    "svg-parser": "^2"
  },
  "devDependencies": {
    "@packages/tsconfig": "*",
    "documentation": "^13",
    "eslint": "^8",
    "eslint-config-kdnj": "^2"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj"
    ],
    "ignorePatterns": [
      "node_modules"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{json,md,svg}": [
      "yarn workspace create-chakra-icons prettier --write"
    ],
    "**/*.{js,cjs,mjs,ts}": [
      "yarn workspace create-chakra-icons eslint --fix"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "keywords": [
    "chakra-ui",
    "cli",
    "generator",
    "javascript",
    "react",
    "react-native",
    "reactjs",
    "svg"
  ],
  "license": "MIT"
}
