{
  "name": "chakra-icons",
  "version": "3.6.9",
  "author": "ri7nz <ri7nz@evilfactory.id>",
  "main": "dist/index.js",
  "scripts": {
    "# general commands": "--------------------------------------------------",
    "build": "./scripts/ngoding",
    "format": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint --fix \"**/*.js\"",
    "#": "-------------------------------------------------------------------"
  },
  "dependencies": {
    "create-chakra-icons": "1.0.3-next.1",
    "change-case": "^4",
    "degit": "^2",
    "glob": "^7",
    "js-yaml": "^4",
    "ora": "^5"
  },
  "devDependencies": {
    "@packages/tsconfig": "*",
    "eslint": "^8",
    "eslint-config-kdnj": "^2"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj"
    ],
    "ignorePatterns": [
      "node_modules"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "yarn workspace chakra-icons prettier --write"
    ],
    "**/*.{js,cjs,mjs,ts}": [
      "yarn workspace chakra-icons eslint --fix"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "license": "MIT"
}
