{
  "name": "@chakra-icons/core",
  "description": "Core internals for @chakra-icons/cli",
  "version": "0.0.0",
  "author": "ri7nz <ri7nz@koding.ninja>",
  "homepage": "https://github.com/kodingdotninja/chakra-icons",
  "repository": "https://github.com/kodingdotninja/chakra-icons.git",
  "bugs": "https://github.com/kodingdotninja/chakra-icons/issues",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "export": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "# general commands": "--------------------------------------------------",
    "build": "tsup",
    "format": "prettier --write \"**/*.{js,json,md,svg}\"",
    "lint": "eslint --fix \"**/*.ts\"",
    "#": "-------------------------------------------------------------------"
  },
  "dependencies": {
    "@babel/generator": "^7",
    "@babel/types": "^7",
    "create-chakra-icons": "^1",
    "fp-ts": "^2"
  },
  "devDependencies": {
    "@packages/tsconfig": "*",
    "@types/degit": "^2",
    "@types/glob": "^7",
    "change-case": "^4",
    "degit": "^2",
    "eslint": "^8",
    "eslint-config-kdnj": "^2",
    "eslint-plugin-fp-ts": "^0.3.0",
    "glob": "^7",
    "tsup": "^5",
    "typescript": "^4"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj",
      "plugin:fp-ts/recommended",
      "plugin:fp-ts/recommended-requiring-type-checking"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{json,md}": [
      "yarn workspace @chakra-icons/core prettier --write"
    ],
    "**/*.{js,cjs,mjs}": [
      "yarn workspace @chakra-icons/core eslint --fix"
    ]
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.mjs"
  ],
  "keywords": [
    "kdnj",
    "kodingdotninja",
    "chakra-icons",
    "create-chakra-icons",
    "cli"
  ],
  "license": "MIT"
}
